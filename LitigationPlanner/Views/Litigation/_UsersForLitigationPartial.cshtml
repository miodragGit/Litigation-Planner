@model List<LitigationPlanner.Data.Models.DataTransferObjects.ApplicationUserDto>


<table class="table">
    <thead class="thead-light">
        <tr>
            <th scope="col">First name</th>
            <th scope="col">Last name</th>
            <th scope="col">Username</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.UserName</td>
            </tr>

        }
    </tbody>
</table>
